<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>Events!</h1>       
    <div class="table-responsive">
    <table class="table table-hover">
        <tr class="info">
              <th>category </th>
              <th>attendees</th>
              <th>organizer</th>
              <th>order</th>
              <th>subcategory</th>
              <th>venue</th>
              <th>event</th>
              <th>user_id</th>
        </tr>

      <% @events.each do |e| %>

        <tr class="active">
            <td><%= e.category %></td>
            <td><%= e.attendees %></td>
            <td><%= e.organizer %></td>
            <td><%= e.order %></td>
            <td><%= e.subcategory %></td>
            <td><%= e.venue %></td>
            <td><%= e.event %></td>
            <td><%= e.user_id %></td>
            <td><%= link_to "Show", event_path(e) %></td>
        </tr>
     <% end %>
       </table>
    </div>
    </div>
</div>