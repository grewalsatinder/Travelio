<br /><br /><br />
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>Shopping!</h1>       
    <div class="table-responsive">
    <table class="table table-hover">
        <tr class="info">
              <th>Suvenir </th>
              <th>Gifts</th>
              <th>T-shirts</th>
              <th>Mugs</th>
        </tr>

      <% @shopping.each do |s| %>

        <tr class="active">
            <td><%= s.suvenir %></td>
            <td><%= s.gifts %></td>
            <td><%= s.tshirts %></td>
            <td><%= s.mugs %></td>
        </tr>
     <% end %>
       </table>
    </div>
    </div>
</div>